urxvt.scrollBar: false
urxvt.shading: 40 

!urxvt.tintColor: #172A47
!urxvt.inheritPixmap: true
urxvt.depth: 32

urxvt.keysym.Control-Shift-V: perl:clipboard:paste
urxvt.keysym.Control-Shift-C: perl:clipboard:copy

urxvt.iso14755: False

urxvt.perl-ext-common: default,clipboard

!Fonts

urxvt.font: xft:Terminess Powerline:pixelsize=14,style=regular
urxvt.boldFont: xft:Source Code Pro:pixelsize=17,style=bold
urxvt.letterSpace: -4

Xft.dpi: 96	
Xft.antialias: true
!Xft.rbga: none
Xft.hintstyle: hintfull
Xft.hinting: true

*.internalBorder: 0


! special
!urxvt.foreground:   #placeholder
!doesnt work because urxvt.transparent and tintcolor

! special
*.foreground: #8aeff4


!Transparancy
urxvt*depth: 32
urxvt*background: [90]#323b47
*.cursorColor:  #8aeff4

